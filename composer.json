{
    "name": "kkarpieszuk/phpstan-wpforms-rules",
    "description": "PHPStan rules to satisfy WPForms code performance and bugs-avoiding standards",
    "type": "phpstan-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Konrad Karpieszuk",
            "email": "kkarpieszuk@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "phpstan/phpstan": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "PhpStanWpformsRules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpStanWpformsRules\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "rules.neon"
            ]
        }
    }
}