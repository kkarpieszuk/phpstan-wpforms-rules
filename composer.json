{
    "name": "kkarpieszuk/phpstan-wpforms-rules",
    "description": "PHPStan rules to satisfy WPForms code performance and bugs-avoiding standards",
    "type": "phpstan-extension",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Konrad Karpieszuk",
            "email": "kkarpieszuk@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "PhpStanWpformsRules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpStanWpformsRules\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "rules.neon"
            ]
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "php-stubs/woocommerce-stubs": "^9.3",
        "szepeviktor/phpstan-wordpress": "^1.3"
    }
}
